public class TestMax{
	
	public static int max(int left, int right){
		return left>right ? left:right;
	}
	
	
	
	
	
	
	
	public static void main(String[] args){
		//3개 정수 중 가장 큰 값 구하기
		int a=3;
		int b=4;
		int c=5;
		
		//4개 정수 중 가장 큰 값
		int d=6;
		//5개 정수 중 가장 큰 값
		int e=7;
		
		System.out.println(max(a,b));
		
	}
	
}