public class LoopLoop{
	public static void main(String[] args){
		for(int i=0; i<5;i++){
			if(i%2==0){
				for(int j =1; j<=5;j++){
					System.out.printf("(%d)",j);
				}
				System.out.println();
			}else{
				for(int k =5; k>0;k--){
					System.out.printf("(%d)",k);
				}
				System.out.println();
			}
		}
		
		
		
		
		
		
